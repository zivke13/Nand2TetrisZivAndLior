/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:

	Nand(a=a , b=b , out=ab);
	Nand(a=a , b=ab ,out=aab);
	Nand(a=b , b=ab , out=bab);
	Nand(a=aab , b=bab , out=out);
	
	
}